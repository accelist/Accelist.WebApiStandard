{
    "$schema": "http://json.schemastore.org/template",
    "author": "Ryan Elian",
    "classifications": [
        "ASP.NET Core",
        "Web API"
    ],
    "identity": "Accelist.WebApiStandard",
    "sourceName": "Accelist.WebApiStandard",
    "name": "Accelist Standard Web Application",
    "shortName": "accelist-standard",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sources": [
        {
            "source": "./",
            "target": "./",
            "include": [
                "**/*"
            ],
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                "**/.template.config/**",
                "**/*.filelist",
                "**/*.user",
